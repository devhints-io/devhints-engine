    "gatsby-link": "^2.0.16",
    "gatsby-mdx": "^0.6.1",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-typescript": "^2.0.12",
    "gatsby-react-router-scroll": "^2.0.7",
    "gatsby-remark-component": "npm:@rstacruz/gatsby-remark-component",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "glob": "^7.1.3",
    "group-by": "^0.0.1",
    "hint.css": "^2.5.1",
    "isotope-layout": "^3.0.5",
    "js-yaml": "^3.12.1",
    "loadjs": "^3.5.5",
    "meow": "^5.0.0",
    "modularscale-sass": "^3.0.8",
    "node-sass": "npm:sass",
    "postcss-apply": "^0.11.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-comments": "^4.0.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-svg-loader": "^2.1.0",
    "recompose": "^0.30.0",
    "rehype-decorate": "1.0.0-rc.5",
    "rehype-react": "^3.1.0",
    "rehype-table-separators": "1.0.0-rc.2",
    "remark": "^10.0.1",
    "remark-cli": "^6.0.1",
    "remark-frontmatter": "^1.3.1",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "responsive-modular-scale.css": "^0.2.0",
    "sanitize.css": "^8.0.0",
    "sass": "^1.16.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-centered": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-notes": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.16",
    "@types/jest": "^23.3.13",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^2.0.1",
    "hast-util-to-html": "^5.0.0",
    "hastscript": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.5",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typed-css-modules": "^0.4.2",
    "typescript": "^3.3.4000",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "keywords": [
    "devhints"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "private": true,
  "scripts": {
    "ci": "run-s -l tsc lint prettier:check test",
    "css_modules:update": "tcm -p 'src/**/*.module.{scss,css}'; prettier --write 'src/**/*.d.ts'",
    "build": "gatsby build && (cd public && bash ../packages/devhints-scripts/postbuild.sh)",
    "postbuild": "run-s storybook:build",
    "develop": "rm -rf .cache; env DEBUG='app:*' gatsby develop --port ${PORT:-19336}",
    "fix": "devhints-validator \"sheets/*.md\" --fix",
    "tsc": "tsc",
    "lint:eslint": "eslint 'packages/*/lib/**/*.js' src",
    "lint": "tslint --project .",
    "prettier:check": "prettier --list-different \"sheets/**/*.md\" \"{src,packages}/**/*.{js,ts,tsx,css}\" \"./*.js\"",
    "prettier:fix": "prettier --write \"sheets/**/*.md\" \"{src,packages}/**/*.{js,ts,tsx,css}\" \"./*.js\"",
    "start": "yarn run develop",
    "storybook:build": "build-storybook --config-dir src/storybook --output-dir public/devhints-storybook",
    "test": "jest",
    "validate": "devhints-validator \"sheets/*.md\""
  },
  "workspaces": [
    "packages/*"
  ]
}
